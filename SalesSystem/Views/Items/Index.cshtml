@model IEnumerable<Item>

@{
    ViewData["Title"] = " سجل الاصناف";
}

<div id="view-all">
    @await Html.PartialAsync("_ViewAll", Model)
</div>
<div >
<div class="modal" tabindex="-1" role="dialog" id="form-modal" >
        <div class="modal-dialog" role="document" >
            <div class="modal-content" style="background-color:#2f533e;color:#f1e1e4">
            <div class="modal-header">
                <h5 class="modal-title"></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
            </div>
        </div>
    </div>
</div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <script src="~/js/stock.js" asp-append-version="true"></script>
    <script src="~/js/item.js" asp-append-version="true"></script>

}

